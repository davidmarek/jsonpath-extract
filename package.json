{
    "name": "jsonpath-selection",
    "displayName": "JSONPath Extract",
    "description": "Use JSONPath to extract parts of your json document.",
    "version": "0.0.1",
    "publisher": "davidmarek",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "json", "jsonpath"
    ],
    "activationEvents": [
        "onCommand:extension.jsonpath"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.jsonpath",
                "title": "JSONPath search"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/jsonpath": "^0.2.0",
        "@types/lodash": "^4.14.109",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "jsonpath": "^1.0.0",
        "lodash": "^4.17.10"
    }
}
