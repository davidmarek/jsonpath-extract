{
    "name": "jsonpath-extract",
    "displayName": "JSONPath Extract",
    "description": "Use JSONPath to extract parts of your json document.",
    "version": "0.1.0",
    "publisher": "davidmarek",
    "license": "SEE LICENSE IN LICENSE",
    "homepage": "https://github.com/davidmarek/jsonpath-extract",
    "repository": {
        "type": "git",
        "url": "https://github.com/davidmarek/jsonpath-extract.git"
    },
    "bugs": {
        "email": "david@marek.me",
        "url": "https://github.com/davidmarek/jsonpath-extract/issues"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "json",
        "jsonpath"
    ],
    "activationEvents": [
        "onCommand:jsonPathExtract.queryToPlainText",
        "onCommand:jsonPathExtract.queryToJson"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "jsonPathExtract.queryToPlainText",
                "title": "Run jsonpath query - plaintext",
                "category": "JPE"
            },
            {
                "command": "jsonPathExtract.queryToJson",
                "title": "Run jsonpath query - json",
                "category": "JPE"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/chai": "^4.1.3",
        "@types/jsonpath": "^0.2.0",
        "@types/lodash": "^4.14.109",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/sinon": "^5.0.0",
        "chai": "^4.1.2",
        "sinon": "^5.0.10",
        "ts-sinon": "^1.0.12",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "jsonpath": "^1.0.0",
        "lodash": "^4.17.10"
    }
}
